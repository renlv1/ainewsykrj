{
  "name": "pinworld",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-diff": "vue-cli-service build --mode development_build",
    "serve-diff": "vue-cli-service serve --mode production"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "cube-ui": "~1.12.1",
    "express": "^4.16.4",
    "normalize": "^0.3.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "swiper": "^6.5.1",
    "vue": "^2.6.6",
    "vue-awesome-swiper": "^2.6.5",
    "vue-count-to": "^1.0.13",
    "vue-i18n": "^8.10.0",
    "vue-loading-template": "^1.3.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "style-resources-loader": "^1.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-cube-ui": "^0.2.4",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-cropper": "^0.4.8",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "vue/no-use-v-if-with-v-for": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "globals": {
      "sc": true
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "not ie <= 11",
    "Android >= 4.0",
    "iOS >= 8"
  ]
}
